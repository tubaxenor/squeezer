{
  "name": "Squeezer",
  "description": "squeeze the good part of your page",
  "background": {
    "scripts": ["popup.js"]
  },
  "version": "0.0.5",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
